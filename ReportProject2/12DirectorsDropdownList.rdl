  <DataSets>
    <DataSet Name="dtsFilmAndDirector">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        tblFilm.FilmName, tblFilm.FilmReleaseDate, tblFilm.FilmDirectorID, tblDirector.DirectorName
FROM            tblFilm INNER JOIN
                         tblDirector ON tblFilm.FilmDirectorID = tblDirector.DirectorID</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
          <DataField>FilmReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="FilmDirectorID">
          <DataField>FilmDirectorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Filters>
        <Filter>
          <FilterExpression>=Fields!FilmDirectorID.Value</FilterExpression>
          <Operator>In</Operator>
          <FilterValues>
            <FilterValue>=Parameters!DirectorID.Value</FilterValue>
          </FilterValues>
        </Filter>
      </Filters>
    </DataSet>
    <DataSet Name="dtsDirectors">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        DirectorID, DirectorName
FROM            tblDirector
ORDER BY DirectorName</CommandText>
      </Query>
      <Fields>
        <Field Name="DirectorID">
          <DataField>DirectorID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DirectorName">
          <DataField>DirectorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
