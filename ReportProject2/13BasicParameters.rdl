  <DataSets>
    <DataSet Name="dtsFilmParameters">
      <Query>
        <DataSourceName>MoviesDS</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@prmFilmname">
            <Value>=Parameters!prmFilmname.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@prmMinLength">
            <Value>=Parameters!prmMinLength.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        FilmName, FilmReleaseDate, FilmRunTimeMinutes
FROM            tblFilm
WHERE        ((FilmName = @prmFilmname) OR
                         (@prmFilmname IS NULL))  AND
                      (FilmRunTimeMinutes &gt;= @prmMinLength OR @prmMinLength IS NULL)</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmReleaseDate">
