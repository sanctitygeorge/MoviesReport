  <DataSets>
    <DataSet Name="dtsStudioCertificationRuntime">
      <Query>
        <DataSourceName>dscMovies</DataSourceName>
        <CommandText>SELECT        tblStudio.StudioName, tblCertificate.CertificateName, tblFilm.FilmRunTimeMinutes
FROM            tblFilm INNER JOIN
                         tblStudio ON tblFilm.FilmStudioID = tblStudio.StudioID INNER JOIN
                         tblCertificate ON tblFilm.FilmCertificateID = tblCertificate.CertificateID</CommandText>
      </Query>
      <Fields>
        <Field Name="StudioName">
          <DataField>StudioName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
