    <DataSet Name="dtsFilmRuntime">
      <Query>
        <DataSourceName>MoviesDS</DataSourceName>
        <CommandText>SELECT        FilmName, FilmRunTimeMinutes
FROM            tblFilm</CommandText>
      </Query>
      <Fields>
        <Field Name="FilmName">
          <DataField>FilmName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="FilmRunTimeMinutes">
          <DataField>FilmRunTimeMinutes</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FilmLengthDescription">
          <Value>=Switch(Fields!FilmRunTimeMinutes.Value&lt;100,"Short",Fields!FilmRunTimeMinutes.Value&lt;150,"Medium",Fields!FilmRunTimeMinutes.Value&lt;200,"Long", TRUE, "Epic")</Value>
        </Field>
        <Field Name="FilmLengthSentence">
          <Value>=Fields!FilmName.Value &amp; " is a " &amp; Fields!FilmLengthDescription.Value &amp; " film"</Value>
        </Field>
      </Fields>
    </DataSet>
